{
  "name": "@milkie/react",
  "version": "0.1.0",
  "description": "Drop-in paywall infrastructure for Next.js apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "paywall",
    "stripe",
    "subscriptions",
    "nextjs",
    "react",
    "saas"
  ],
  "author": "Andrew Cho",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/akcho/milkie"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.545.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  }
}
